							TITLE_EXPR='getBooleanParam("show.linkTitle")
?
  (nsURI = getStringParam("$nsURI")) != "" ?
    "Namespace {" + nsURI + "}" : "No Namespace"
: ""'
							TARGET_FRAME_EXPR='"detailFrame"'
							HKEYS={
								'getStringParam("$nsURI")';
								'"detail"';
