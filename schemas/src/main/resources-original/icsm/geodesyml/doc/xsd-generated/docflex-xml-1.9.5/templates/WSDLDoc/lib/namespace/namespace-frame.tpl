		DESCR='namespace URI'
		FMT={
			par.style='s4';
			par.option.nowrap='true';
		}
		<AREA>
			<CTRL_GROUP>
				<CTRLS>
					<DATA_CTRL>
						FORMULA='(nsURI = getStringParam("$nsURI")) != "" ? nsURI : "{no namespace}"'
